555:
556:/* Lets do AB at the same time as RAS asserts, but slightly delayed. */
557:DRAM_MEM_AB.d = 'b'0;         
558:DRAM_MEM_AB.ck = !DRAM_MEM_RAS;
559:DRAM_MEM_AB.ap = DRAM_CS # CPU_AS # !CPU_RESET;

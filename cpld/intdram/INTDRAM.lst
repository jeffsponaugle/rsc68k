222:RESET_REQ.d = CPU_D7.io;
223:/* flip flop will clock in from databus on rising edge of INTC_CS if A1=1, A2=1, RW=0 */
224:RESET_REQ.ck = INTC_CS # !CPU_A1 # !CPU_A2 # CPU_RW;

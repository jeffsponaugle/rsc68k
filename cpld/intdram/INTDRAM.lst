598:REFRESH_COUNTER_RESET.d = !(dram_access_timer_field:[1,2]);
599:REFRESH_COUNTER_RESET.ck = !CLK_32M;
600:REFRESH_COUNTER_RESET.ar = !CPU_RESET;

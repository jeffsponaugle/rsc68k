497:
498:/* 
499:   The field makes it easier to build logic based on the combined value
500:   of the counter values.  
501:*/
502:
503:FIELD dram_access_timer_field = [DRAM_ACCESS_TIMER2..0];
504:
505:/* ################## DRAM ACCESS SECTION ######################### */
506:
507:/* 
508:   mid flip flop for dram /RAS line.  Active for counter values 2,3 
509:   Latched by rising edge of CLK_32M. Held in preset(==1) when DRAM_CS is high 
510:   ( no dram access in progress )
511:
512:   output flip flop for DRAM /RAS line. fed from _PRE output Latches on 
513:   falling edge of CLK_32M. Held in preset(==1) when AS is high .  Adds 1/2 delay.
514:*/
515:
516:DRAM_MEM_RAS.ck = CLK_16M;
517:DRAM_MEM_RAS.ap = DRAM_CS # CPU_AS # !CPU_RESET;
518:DRAM_MEM_RAS.d = !(dram_access_timer_field:[1]);
519:
520:/* 
521:   mid flip flop for dram /RAS line.  Active for counter values 3,4,5
522:   Latched by rising edge of CLK_32M. Held in preset(==1) when DRAM_CS is high 
523:   ( no dram access in progress )
524:
525:   output flip flop for DRAM /RAS line. fed from _PRE output Latches on 
526:   falling edge of CLK_32M. Held in preset(==1) when AS is high  Adds 1/2 delay.
527:*/
528:/* - Working at 12Mhz
529:      DRAM_MEM_CAS.d = !(dram_access_timer_field:[2,3,4]);
530:      DRAM_MEM_CAS.ck = !CLK_32M;
531:      DRAM_MEM_CAS.ap = CPU_AS # !CPU_RESET # DRAM_CS;
532:*/
533:
534:/* Move CAS back 1/2 cycle by using 1, plus 1/2 delay */
535:DRAM_MEM_CAS_PRE.d = !(dram_access_timer_field:[1,2,3,4]);
536:DRAM_MEM_CAS_PRE.ck = !CLK_32M;
537:DRAM_MEM_CAS_PRE.ap = CPU_AS # !CPU_RESET # DRAM_CS;
538:
539:DRAM_MEM_CAS.d = DRAM_MEM_CAS_PRE;


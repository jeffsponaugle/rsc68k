218:/* Interupt Mask Registers -  
219:   ---------------------------------------------------------------------------------------------------
220:    
221:    0 = allow interrupt, 1 = block interrupt */
222:/*
223:
224:Write to FF0700 Upper 8 bits: 
225:                  bit 7 - DEBUG_IN_MASK
226:                  bit 6 - IRQL6A_MASK
227:                  bit 5 - IRQL6B_MASK
228:                  bit 4 - IRQL5A_MASK
229:                  bit 3 - IRQL5B_MASK
230:                  bit 2 - IRQL4A_MASK
231:                  bit 1 - KEYBOARD_IRQ_MASK
232:                  bit 0 - Unused
233:
234: Write to FF0702 Upper 8 bits:
235:                  bit 7 - IRQL3A_MASK
236:                  bit 6 - IRQL3B_MASK
237:                  bit 5 - IRQL2A_MASK
238:                  bit 4 - IRQL2B_MASK
239:
240: WRITE to FF0704 Upper 8 bits:
241:                  bit 7 - CPU_1_RESET        ( defaults to 0, 0 holds CPU1 in reset/halt )
242:                  bit 6 - CPU_1_HALT         (defaults to 0, 0 hold CPU1 in reset/halt)
243:                  bit 5 - CPU_1 Interrupt 1  (defaults to 0, 1 triggers interrupt)
244:                  bit 4 - CPU_1 Interrupt 2  (defaults to 0, 1 triggers interrupt)
245:                  bit 3 - CPU_1 Barrier Flag (defaults to 0)
246:                  bit 2
247:                  bit 1
248:                  bit 0
249:
250: READ from FF0704 Upper 8 bits:  (This will read as all 0's for the CPU0)
251:                  bit 3 - CPU_ID bit 3
252:                  bit 2 - CPU_ID bit 2
253:                  bit 1 - CPU_ID bit 1 
254:                  bit 0 - CPU_ID bit 0
255:
256: Write to FF0706 Upper 8 bits:
257:                  bit 7 - REQUEST_RESET (Set to 1 to force SYS_RESET)
258:                  bit 0 - POWERCTRL pin state value
259:                  bit 1 - POWERCTRL pin output enable
260:
261:*/
262:
263:REG_WRITE_ITC0 =  INTC_CS # CPU_A1 # CPU_A2 # CPU0_RW;   /* WRITE Address 0xFF0700 */
264:REG_WRITE_ITC1 =  INTC_CS # !CPU_A1 # CPU_A2 # CPU0_RW;  /* WRITE Address 0xFF0702 */
265:REG_WRITE_ITC2 =  INTC_CS # CPU_A1 # !CPU_A2 # CPU0_RW;   /* WRITE Address 0xFF0704 */
266:REG_WRITE_ITC3 =  INTC_CS # !CPU_A1 # !CPU_A2 # CPU0_RW; /* WRITE Address 0xFF0706 */
267:REG_READ_ITC2  =  INTC_CS # CPU_A1 # !CPU_A2 # !CPU0_RW;  /* READ  Address 0xff0704 */

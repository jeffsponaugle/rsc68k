389:/* 
390:   Active low grouping of external active low interrupts.  If any of these inputs are low,
391:   the group is low.  That group is used to select the interrupt level that goes to the CPU.
392:*/
393:
394:IRQL7_INT = DEBUG_IN_LATCHED_PE;
395:IRQL6_INT = (IRQL6A_PE & IRQL6B_PE) # !IRQL7_INT;
396:IRQL5_INT = (IRQL5A_PE & IRQL5B_PE) # !IRQL6_INT # !IRQL7_INT;
397:IRQL4_INT = (IRQL4A_PE & IRQL4B_PE) # !IRQL5_INT # !IRQL6_INT # !IRQL7_INT;
398:IRQL3_INT = (IRQL3A_PE & IRQL3B_PE) # !IRQL4_INT # !IRQL5_INT # !IRQL6_INT # !IRQL7_INT;
399:IRQL2_INT = (IRQL2A_PE) # !IRQL3_INT #!IRQL4_INT # !IRQL5_INT # !IRQL6_INT # !IRQL7_INT;
400:
401:/* 
402:   CPU0_IPL Drive using 8:3 priority encoder but only for 4 sources.   This output is driven from the above grouped signals.  
403:   This picks the highest priority interrupt level and encodes that to the CPU.  The CPU will respond
404:   to any value other than 111 by doing an interrupt ack cycle.
405:*/   
406:
407:CPU0_IPL0 = !(!IRQL3_INT # !IRQL5_INT # !IRQL7_INT);
408:CPU0_IPL1 = !(!IRQL3_INT # !IRQL6_INT # !IRQL7_INT # !IRQL2_INT);
409:CPU0_IPL2 = !(!IRQL4_INT # !IRQL5_INT # !IRQL6_INT # !IRQL7_INT);

335:/* Power Control Logic */
336:
337:/* 
338:   Latch the POWERCTL_STATE register if a write to 0xFF0706 happens, and use bit 0 for the STATE.  
339:   On RESET this will be set to 0.
340:   Latch the POWERCTL_OE register if a write to 0xFF0706 happens, and use bit 1 for the OE.
341:   On RESET this will be set to 0.   A value of 0 indicated output is not enabled.
342:*/
343:
344:RESET_REQ.d = CPU0_D7.io;
345:/* flip flop will clock in from databus on rising edge of INTC_CS if A1=1, A2=1, RW=0 */
346:RESET_REQ.ck = REG_WRITE_ITC3;
347:RESET_REQ.ar = !SYS_RESET;
348:
349:POWERCTL_OE.d = CPU0_D1.io;
350:POWERCTL_OE.ck = REG_WRITE_ITC3;
351:POWERCTL_OE.ar = !SYS_RESET;
352:
353:POWERCTL_STATE.d = CPU0_D0.io;
354:POWERCTL_STATE.ck = REG_WRITE_ITC3;
355:POWERCTL_STATE.ar = !SYS_RESET;
356:POWERCTL_STATE.oe = POWERCTL_OE;

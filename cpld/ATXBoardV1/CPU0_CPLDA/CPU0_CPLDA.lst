108:DTACK_MEMORY_0W = !DRAM_CS # !FLASH_LOW_CS # !FLASH_HIGH_CS # !NIC_CS # !EXP_CS_C 
109:                  # !SEVENSEG_LATCH_CS # !UART_CSA # !UART_CSB # !IDE_CSA # !IDE_CSB
110:                  # !LED_LATCH_CS # !RTS_CS # !PTC_CS  # !K8042_CS
111:                  # !FLASH_CONFIG_CLOCK # !ITC_CS;

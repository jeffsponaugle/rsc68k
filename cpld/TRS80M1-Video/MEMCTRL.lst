219:/*
220:REGION Congig Register:
221:        bit 7 -    HARDWARE ENABLED in the PAGE. 1=HARDWARE ON, 0=HARDWARE OFF
222:        bit 6 -    0=SRAM, 1=FLASH
223:        bit 5,4,3 
224:            2,1,0  6-bit page selection (16K page)
225:
226:        REGION0 defaults to FLASH, and Hardware Enabled.
227:        REGION1,2,3 defaults to SRAM.
228:
229:*/
230:
231:[REGION0_CONFIG7..0].ck = Z80_IOWR # !(cpu_address_bus:'h'XX70);
232:[REGION0_CONFIG7..0].d = [Z80_D7..0].io;
233:[REGION0_CONFIG5..0].ar = !SYS_RESET;
234:REGION0_CONFIG6.ap = !SYS_RESET;
235:REGION0_CONFIG7.ap = !SYS_RESET;
236:
237:[REGION1_CONFIG7..0].ck = Z80_IOWR # !(cpu_address_bus:'h'XX71);
238:[REGION1_CONFIG7..0].d = [Z80_D7..0].io;
239:[REGION1_CONFIG5..0].ar = !SYS_RESET;
240:REGION1_CONFIG6.ar = !SYS_RESET;
241:REGION1_CONFIG7.ar = !SYS_RESET;
242:
243:[REGION2_CONFIG7..0].ck = Z80_IOWR # !(cpu_address_bus:'h'XX72);
244:[REGION2_CONFIG7..0].d = [Z80_D7..0].io;
245:[REGION2_CONFIG5..0].ar = !SYS_RESET;
246:REGION2_CONFIG6.ar = !SYS_RESET;
247:REGION2_CONFIG7.ar = !SYS_RESET;
248:
249:[REGION3_CONFIG7..0].ck = Z80_IOWR # !(cpu_address_bus:'h'XX73);
250:[REGION3_CONFIG7..0].d = [Z80_D7..0].io;
251:[REGION3_CONFIG5..0].ar = !SYS_RESET;
252:REGION3_CONFIG6.ar = !SYS_RESET;
253:REGION3_CONFIG7.ar = !SYS_RESET;
254:
255:/* System Configuration Registers - 
256:        bit 0,1:  CPU Speed
257:                    00 = Divide/16
258:                    01 = Divide/8
259:                    10 = Divide/4

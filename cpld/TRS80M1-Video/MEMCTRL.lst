152:REGION1_CS = !(ISMEMORYACCESS & (cpu_address_bus:'h'4XXX
153:                               # cpu_address_bus:'h'5XXX
154:                               # cpu_address_bus:'h'6XXX
155:                               # cpu_address_bus:'h'7XXX
156:                               ));
157:REGION2_CS = !(ISMEMORYACCESS & (cpu_address_bus:'h'8XXX

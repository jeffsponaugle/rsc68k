static ELCDErr TerminalWidgetAlloc(SWidget *psWidget,
								   WIDGETHANDLE eHandle)
{
	STerminal *psTerminal;

	GCASSERT(psWidget);
	psTerminal = MemAlloc(sizeof(*psWidget->uWidgetSpecific.psTerminal));
	psWidget->uWidgetSpecific.psTerminal = psTerminal;
	if (NULL == psTerminal)
	{
		return(LERR_NO_MEM);
	}
	else
	{
		return(LERR_OK);
	}
}

static ELCDErr TerminalWidgetFree(SWidget *psWidget)
{
	STerminal *psTerminal;

	GCASSERT(psWidget);
	GCASSERT(psWidget->uWidgetSpecific.psTerminal);
	psTerminal = psWidget->uWidgetSpecific.psTerminal;

	if (psTerminal->pu8CharMap)
	{
		MemFree(psTerminal->pu8CharMap);
		psTerminal->pu8CharMap = NULL;
	}

	GCFreeMemory(psWidget->uWidgetSpecific.psTerminal);
	psWidget->uWidgetSpecific.psTerminal = NULL;
	return(LERR_OK);	
}

#define	VIDATTR_BLINK		0x01
#define VIDATTR_INVERSE		0x02	

static void TerminalDrawFont(STerminal *psTerminal,
							 UINT16 *pu16PixelLocation,
							 UINT32 u32Pitch,
							 UINT32 u32CharColorAttrib,
							 BOOL bCursor)
{
	UINT8 const *pu8SrcData;
	UINT8 u8Loop;
	UINT8 u8Loop2;
	UINT16 u16Foreground;
	UINT16 u16Background;
	UINT8 u8Attribute;
	UINT8 u8Color;
	BOOL bShow = TRUE;

	pu8SrcData = &sg_u8VideoFont[(u32CharColorAttrib & 0xff) << 3];
	u8Attribute = (UINT8) (u32CharColorAttrib >> 16);
	u8Color = (UINT8) (u32CharColorAttrib >> 8);

	// Inverse?
	if (u8Attribute & VIDATTR_INVERSE)
	{
		u16Foreground = sg_u16ColorMap[u8Color >> 4];
		u16Background = sg_u16ColorMap[u8Color & 0xf];
	}
	else
	{
		u16Foreground = sg_u16ColorMap[u8Color & 0x0f];
		u16Background = sg_u16ColorMap[u8Color >> 4];
	}

	if (u8Attribute & VIDATTR_BLINK)
	{
		bShow = psTerminal->bBlinkShow;
	}

	// This means we're drawing a cursor
	if (bCursor)
	{
		u16Background = sg_u16ColorMap[7];
		u16Foreground = sg_u16ColorMap[0];
		bShow = TRUE;
	}

	// If this is true, then show the font
	if (bShow)
	{
		// 8 pixels wide, so subtract this so we position to the next line accordingly
		for (u8Loop = 0; u8Loop < 8; u8Loop++)
		{
			UINT8 u8Data;

			u8Data = *pu8SrcData;
			++pu8SrcData;

			for (u8Loop2 = 0; u8Loop2 < 8; u8Loop2++)
			{
				if (u8Data & 0x80)
				{
					*pu16PixelLocation = u16Foreground;
					*(pu16PixelLocation + u32Pitch) = u16Foreground;
				}
				else
				{
					*pu16PixelLocation = u16Background;
					*(pu16PixelLocation + u32Pitch) = u16Background;
				}

				++pu16PixelLocation;
				u8Data <<= 1;
			}

			pu16PixelLocation += ((u32Pitch - 8) + u32Pitch);
		}
	}
	else
	{
		// Just draw the background
		// 8 pixels wide, so subtract this so we position to the next line accordingly
		for (u8Loop = 0; u8Loop < 8; u8Loop++)
		{
			UINT8 u8Data;

			u8Data = *pu8SrcData;
			++pu8SrcData;

			for (u8Loop2 = 0; u8Loop2 < 8; u8Loop2++)
			{
				*pu16PixelLocation = u16Background;
				*(pu16PixelLocation + u32Pitch) = u16Background;
				++pu16PixelLocation;
				u8Data <<= 1;
			}

			pu16PixelLocation += ((u32Pitch - 8) + u32Pitch);
		}
	}
}

static void TerminalEraseCursor(STerminal *psTerminal,
								SWindow *psWindow,
								SWidget *psWidget)
{
	UINT8 *pu8SrcData;
	UINT16 *pu16Target;
	SImage *psWindowImage;

	// Compute our source data
	pu8SrcData = psTerminal->pu8CharMap + (psTerminal->u32YCursorPos * psTerminal->u32XSize * 3) + (psTerminal->u32XCursorPos * 3);
	psWindowImage = psWindow->psWindowImage;

	// Compute our draw position
	pu16Target = psWindowImage->pu16ImageData + (((psTerminal->u32YCursorPos << 4) + (psWidget->s32YPos)) * psWindowImage->u32Pitch) + (psTerminal->u32XCursorPos << 3);

	TerminalDrawFont(psTerminal,
						pu16Target,
						psWindowImage->u32Pitch,
						*((UINT32 *) pu8SrcData),
						FALSE);

	WindowUpdateRegion(psWidget->eParentWindow,
						psWidget->s32XPos + (psTerminal->u32XCursorPos << 3),
						psWidget->s32YPos + (psTerminal->u32YCursorPos << 4),
						8,
						16);
}

// Draw the cursor at the specified location
static void CursorDraw(SWidget *psWidget,
					   BOOL bClearOnly)
{
	STerminal *psTerminal;
	SWindow *psWindow;
	UINT8 *pu8SrcData;
	SImage *psWindowImage;
	UINT16 *pu16Target;

	GCASSERT(psWidget);
	psTerminal = psWidget->uWidgetSpecific.psTerminal;
	GCASSERT(psTerminal);
	psWindow = WindowGetPointer(psWidget->eParentWindow);
	GCASSERT(psWindow);

	// Compute our source data
	pu8SrcData = psTerminal->pu8CharMap + (psTerminal->u32YCursorPos * psTerminal->u32XSize * 3) + (psTerminal->u32XCursorPos * 3);
	psWindowImage = psWindow->psWindowImage;

	// Compute our draw position
	pu16Target = psWindowImage->pu16ImageData + (((psTerminal->u32YCursorPos << 4) + (psWidget->s32YPos)) * psWindowImage->u32Pitch) + (psTerminal->u32XCursorPos << 3);

	if ((bClearOnly) || (psTerminal->u32CursorTimer < CURSOR_ON_TIME))
	{
		bClearOnly = TRUE;
	}
	else
	{
		bClearOnly = FALSE;
	}

	// If the cursor is shut off, don't draw it
	if (FALSE == psTerminal->bCursorOn)
	{
		bClearOnly = FALSE;
	}

	TerminalDrawFont(psTerminal,
					 pu16Target,
					 psWindowImage->u32Pitch,
					 *((UINT32 *) pu8SrcData),
					 bClearOnly);

	WindowUpdateRegion(psWidget->eParentWindow,
					   psWidget->s32XPos + (psTerminal->u32XCursorPos << 3),
					   psWidget->s32YPos + (psTerminal->u32YCursorPos << 4),
					   8,
					   16);
}

static void TerminalPaint(SWidget *psWidget,
						  BOOL bLock)
{
	STerminal *psTerminal;
	SWindow *psWindow;
	UINT8 *pu8SrcData;
	SImage *psWindowImage;
	UINT32 u32X;
	UINT32 u32Y;
	UINT16 *pu16Target;

	GCASSERT(psWidget);
	psTerminal = psWidget->uWidgetSpecific.psTerminal;
	GCASSERT(psTerminal);
	psWindow = WindowGetPointer(psWidget->eParentWindow);
	GCASSERT(psWindow);

	pu8SrcData = psTerminal->pu8CharMap;
	psWindowImage = psWindow->psWindowImage;

	// Time to repaint everything
	for (u32Y = 0; u32Y < psTerminal->u32YSize; u32Y++)
	{
		pu16Target = psWindowImage->pu16ImageData + (((u32Y << 4) + (psWidget->s32YPos)) * psWindowImage->u32Pitch) + (psWidget->s32XPos);

		for (u32X = 0; u32X < psTerminal->u32XSize; u32X++)
		{
			TerminalDrawFont(psTerminal,
							 pu16Target,
							 psWindowImage->u32Pitch,
							 *((UINT32 *) pu8SrcData),
							 FALSE);
			pu16Target += 8;
			pu8SrcData += 3;
		}
	}

	// Go draw the cursor (if applicable)
	CursorDraw(psWidget,
			   FALSE);

	WindowUpdateRegion(psWidget->eParentWindow,
					   psWidget->s32XPos,
					   psWidget->s32YPos,
					   psWidget->u32XSize,
					   psWidget->u32YSize);
}

static void TerminalSetFocus(SWidget *psWidget,
							 BOOL bInFocus)
{
	STerminal *psTerminal;

	GCASSERT(psWidget);
	psTerminal = psWidget->uWidgetSpecific.psTerminal;
	GCASSERT(psTerminal);

	// Time to repaint everything
	DebugOut("Focus = %u\n", bInFocus);
	if (FALSE == bInFocus)
	{
		// No longer in focus. Undraw the cursor if there is one.
		CursorDraw(psWidget,
				   TRUE);
	}
	else
	{
		// Now in focus. Draw the cursor if there is one
		CursorDraw(psWidget,
				   FALSE);
	}
}

static void TerminalKeystroke(SWidget *psWidget,
							  EGCCtrlKey eGCKey,
							  LEX_CHAR eUnicode,
							  BOOL bPressed)
{
	STerminal *psTerminal;

	GCASSERT(psWidget);
	psTerminal = psWidget->uWidgetSpecific.psTerminal;
	GCASSERT(psTerminal);

	if ((psTerminal->pCallback) && (bPressed))
	{
		if (EGCKey_Delete == eGCKey)
		{
			eGCKey = EGCKey_Unicode;
			eUnicode = 127;
		}

		if (EGCKey_Left == eGCKey)
		{
			eGCKey = EGCKey_Unicode;
			eUnicode = 0;
		}
		if (EGCKey_Right == eGCKey)
		{
			eGCKey = EGCKey_Unicode;
			eUnicode = 1;
		}
		if (EGCKey_Up == eGCKey)
		{
			eGCKey = EGCKey_Unicode;
			eUnicode = 2;
		}
		if (EGCKey_Down == eGCKey)
		{
			eGCKey = EGCKey_Unicode;
			eUnicode = 4;
		}
		if (EGCKey_Home == eGCKey)
		{
			eGCKey = EGCKey_Unicode;
			eUnicode = 5;
		}
		if (EGCKey_End == eGCKey)
		{
			eGCKey = EGCKey_Unicode;
			eUnicode = 6;
		}

		// Allow backspace
		if ((EGCKey_Backspace == eGCKey) ||
			(EGCKey_Unicode == eGCKey))
		{
			psTerminal->pCallback(psTerminal,
								  eUnicode);
		}
	}
}

static void TerminalAnimation(SWidget *psWidget,
							  UINT32 u32TickTime)
{
	STerminal *psTerminal;
	SWindow *psWindow;
	BOOL bCommit = FALSE;

	GCASSERT(psWidget);
	psTerminal = psWidget->uWidgetSpecific.psTerminal;
	GCASSERT(psTerminal);
	psWindow = WindowGetPointer(psWidget->eParentWindow);
	GCASSERT(psWindow);

	// Blinky cursor time! Maybe.
	if (psTerminal->bCursorOn)
	{
		BOOL bSetCursorState = FALSE;

		// Cursor is on. Let's see if we blinky or not.
		psTerminal->u32CursorTimer += u32TickTime;

		if (psTerminal->u32CursorTimer >= CURSOR_TOTAL_TIME)
		{
			// Turn on cursor
			psTerminal->u32CursorTimer -= CURSOR_TOTAL_TIME;
			bSetCursorState = TRUE;
		}
		else
		if ((psTerminal->u32CursorTimer <= CURSOR_ON_TIME) && (FALSE == psTerminal->bCursorState))
		{
			// Turn on cursor character
			bSetCursorState = TRUE;
		}
		else
		if ((psTerminal->u32CursorTimer > CURSOR_ON_TIME) && (psTerminal->bCursorState))
		{
			// Turn off cursor character
			bSetCursorState = TRUE;
		}

		if (bSetCursorState)
		{
			TerminalEraseCursor(psTerminal,
								psWindow,
								psWidget);
			bCommit = TRUE;
		}
	}

	// Blinky text time!
	psTerminal->u32BlinkCounter += u32TickTime;
	if (psTerminal->u32BlinkCounter >= TEXT_BLINK_RATE)
	{
		psTerminal->u32BlinkCounter -= TEXT_BLINK_RATE;
		if (psTerminal->bBlinkShow)
		{
			psTerminal->bBlinkShow = FALSE;
		}
		else
		{
			psTerminal->bBlinkShow = TRUE;
		}

		TerminalPaint(psWidget,
					  TRUE);
		bCommit = TRUE;
	}

	if (bCommit)
	{
		WindowUpdateRegionCommit();
	}
}


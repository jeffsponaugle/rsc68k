ELCDErr TerminalCreate(TERMINALHANDLE *peTerminalHandle,
					   WINDOWHANDLE eWindowHandle,
					   INT32 s32XPos,
					   INT32 s32YPos,
					   UINT32 u32XCharSize,
					   UINT32 u32YCharSize)
{
	ELCDErr eLCDErr = LERR_OK;
	SWidget *psWidget = NULL;
	SWindow *psWindow = NULL;
	STerminal *psTerminal = NULL;
	UINT32 u32Loop;

	eLCDErr = WidgetAllocateHandle((WIDGETHANDLE *) peTerminalHandle,
								   WIDGET_TERMINAL,
								   eWindowHandle,
								   &psWidget);

	RETURN_ON_FAIL(eLCDErr);

	psWindow = WindowGetPointer(eWindowHandle);
	GCASSERT(psWindow);

	psTerminal = psWidget->uWidgetSpecific.psTerminal;
	GCASSERT(psTerminal);

	// Set up the basic widget stuff
	psWidget->s32XPos = s32XPos;
	psWidget->s32YPos = s32YPos;
	psWidget->u32XSize = u32XCharSize << 3;
	psWidget->u32YSize = u32YCharSize << 4;
	psWidget->bWidgetHidden = TRUE;
	psWidget->bWidgetEnabled = TRUE;

	// Set up basic character junk here
	psTerminal->u32XSize = u32XCharSize;
	psTerminal->u32YSize = u32YCharSize;
	psTerminal->pu8CharMap = MemAlloc(u32XCharSize * u32YCharSize * 3);
	if (NULL == psTerminal->pu8CharMap)
	{
		(void) WidgetDestroyByHandle((WIDGETHANDLE *) peTerminalHandle,
									 WIDGET_TERMINAL);
		return(LERR_OUT_OF_MEMORY);
	}

	// Turn on the cursor by default
	psTerminal->bCursorOn = TRUE;
	psTerminal->bCursorState = TRUE;

	// Set up character absorption
	psTerminal->eCharState = ECHARSTATE_WAIT_ESCAPE;
	psTerminal->u32RXLen = 0;

	// Set up attributes and colors
	psTerminal->u8Color = 0x07;
	psTerminal->u8Attribute = 0;

	// Run through the entire character map and set everything to white foreground,
	// black background, and spaces. For a clear screen.
	for (u32Loop = 0; u32Loop < (u32XCharSize * u32YCharSize); u32Loop++)
	{
		psTerminal->pu8CharMap[u32Loop * 3] = ' ';
		psTerminal->pu8CharMap[(u32Loop * 3) + 1] = psTerminal->u8Color;	// White foreground, black background
		psTerminal->pu8CharMap[(u32Loop * 3) + 2] = psTerminal->u8Attribute; // No attributes
	}

	eLCDErr = WindowAnimationListAdd(eWindowHandle,
									 psWidget);
	return(eLCDErr);
}


static BOOL TerminalPutDisplayableChar(STerminal *psTerminal,
									   SWindow *psWindow,
									   SWidget *psWidget,
									   UINT8 u8Char)
{
	BOOL bScroll = FALSE;

	if ('\r' == u8Char)
	{
		// Back to the beginning of the line
		psTerminal->u32XCursorPos = 0;
	}
	else
	if ('\n' == u8Char)
	{
nextLine:
		// Next line?
		psTerminal->u32YCursorPos++;
		if (psTerminal->u32YCursorPos >= psTerminal->u32YSize)
		{
			bScroll = TRUE;
			psTerminal->u32YCursorPos--;
		}
	}
	else
	if ('\t' == u8Char)
	{
		// Tab?
		psTerminal->u32XCursorPos = (psTerminal->u32XCursorPos & 0xffffff8) + 8;
		if (psTerminal->u32XCursorPos >= psTerminal->u32XSize)
		{
			psTerminal->u32XCursorPos = 0;
			goto nextLine;
		}
	}
	else
	if ('\b' == u8Char)
	{
		if (0 == psTerminal->u32XCursorPos)
		{
			if (0 == psTerminal->u32YCursorPos)
			{
				// Don't do anything
			}
			else
			{
				// Set cursor at the end of the prior line
				psTerminal->u32XCursorPos = psTerminal->u32XSize - 1;
				psTerminal->u32YCursorPos--;
			}
		}
		else
		{
			psTerminal->u32XCursorPos--;
		}
	}
	else
	{
		UINT8 *pu8SrcData;
		UINT16 *pu16Target;
		SImage *psWindowImage;

		// Compute our source data
		pu8SrcData = psTerminal->pu8CharMap + (psTerminal->u32YCursorPos * psTerminal->u32XSize * 3) + (psTerminal->u32XCursorPos * 3);
		*pu8SrcData = u8Char;
		*(pu8SrcData + 1) = psTerminal->u8Color;
		*(pu8SrcData + 2) = psTerminal->u8Attribute;

		psWindowImage = psWindow->psWindowImage;

		// Compute our draw position
		pu16Target = psWindowImage->pu16ImageData + (((psTerminal->u32YCursorPos << 4) + (psWidget->s32YPos)) * psWindowImage->u32Pitch) + (psTerminal->u32XCursorPos << 3);

		TerminalDrawFont(psTerminal,
						 pu16Target,
						 psWindowImage->u32Pitch,
						 *((UINT32 *) pu8SrcData),
						 FALSE);

		WindowUpdateRegion(psWidget->eParentWindow,
						   psWidget->s32XPos + (psTerminal->u32XCursorPos << 3),
						   psWidget->s32YPos + (psTerminal->u32YCursorPos << 4),
						   8,
						   16);

		// Advance the cursor
		psTerminal->u32XCursorPos++;
		if (psTerminal->u32XCursorPos >= psTerminal->u32XSize)
		{
			psTerminal->u32XCursorPos = 0;
			goto nextLine;
		}
	}

	// Do we scroll? If so, it's time for a scroll plus massive repaint
	if (bScroll)
	{
		UINT8 *pu8Data;
		UINT32 u32Loop = 0;

		// Scroll everything up
		memcpy((void *) psTerminal->pu8CharMap,
			   (void *) (psTerminal->pu8CharMap + (psTerminal->u32XSize * 3)),
			   (psTerminal->u32XSize * (psTerminal->u32YSize - 1)) * 3);

		// On the last line, fill it in with spaces and the current attributes

		pu8Data = psTerminal->pu8CharMap + ((psTerminal->u32XSize * (psTerminal->u32YSize - 1) * 3));
		u32Loop = psTerminal->u32XSize;
		while (u32Loop--)
		{
			*pu8Data = ' ';		// Space
			++pu8Data;
			*pu8Data = psTerminal->u8Color;	// Current color
			++pu8Data;
			*pu8Data = psTerminal->u8Attribute;	// Current attribute
			++pu8Data;
		}

		TerminalPaint(psWidget,
					  FALSE);
	}

	return(bScroll);
}

static BOOL TerminalAbsorbNumber(UINT8 **ppu8Char,
								 UINT32 *pu32Value)
{
	UINT8 u8CharsAbsorbed = 0;

	*pu32Value = 0;

	while ((**ppu8Char >= '0') &&
		   (**ppu8Char <= '9'))
	{
		*pu32Value *= 10;
		*pu32Value += ((**ppu8Char) - '0');
		(*ppu8Char)++;
		++u8CharsAbsorbed;
	}

	if (0 == u8CharsAbsorbed)
	{
		return(FALSE);
	}
	else
	{
		return(TRUE);
	}
}

static UINT8 sg_u8ANSIColorMap[] = 
{
	0x00,
	0x04,
	0x02,
	0x06,
	0x01,
	0x05,
	0x03,
	0x07
};

static BOOL TerminalProcessEscapeSequence(STerminal *psTerminal,
										  SWindow *psWindow,
										  SWidget *psWidget,
										  UINT8 u8Char)
{
	UINT8 *pu8RXChar = psTerminal->u8EscapeRXBuffer;
	BOOL bResult;
	BOOL bScrolled = FALSE;
	UINT32 u32Loop;

	if ('J' == u8Char)
	{
		UINT32 u32Number;

		// Attributes!

		bResult = TerminalAbsorbNumber(&pu8RXChar,
									   &u32Number);

		if (bResult)
		{
			if (2 == u32Number)
			{
				// Clear screen and home cursor
				for (u32Loop = 0; u32Loop < (psTerminal->u32XSize * psTerminal->u32YSize); u32Loop++)
				{
					psTerminal->pu8CharMap[u32Loop * 3] = ' ';
					psTerminal->pu8CharMap[(u32Loop * 3) + 1] = psTerminal->u8Color;	// White foreground, black background
					psTerminal->pu8CharMap[(u32Loop * 3) + 2] = psTerminal->u8Attribute; // No attributes
				}

				psTerminal->u32XCursorPos = 0;
				psTerminal->u32YCursorPos = 0;

				TerminalPaint(psWidget,
							  FALSE);
			}
		}
	}

	if (('h' == u8Char) || ('l' == u8Char))
	{
		UINT32 u32Number;

		// Attributes!

		bResult = TerminalAbsorbNumber(&pu8RXChar,
									   &u32Number);

		if ((bResult) && (25 == u32Number))
		{
			if ('h' == u8Char)
			{
				psTerminal->bCursorOn = TRUE;
			}
			else
			{
				psTerminal->bCursorOn = FALSE;
			}
		}
	}


	if ('m' == u8Char)
	{
		UINT32 u32Number;

		// Attributes!

		while (*pu8RXChar)
		{
			bResult = TerminalAbsorbNumber(&pu8RXChar,
										   &u32Number);

			if (FALSE == bResult)
			{
				// Couldn't absorb a number for some reason
				break;
			}

			// Now react to the number
			if (0 == u32Number)
			{
				// Reset all attributes.
				psTerminal->u8Attribute = 0;	// No blink/inverse
				psTerminal->u8Color = 0x07;		// Mild white
				psTerminal->bCursorOn = TRUE;	// Turn on the cursor again
			}
			else
			if (1 == u32Number)
			{
				psTerminal->u8Color |= 0x08;	// Set high intensity foreground
			}
			else
			if (2 == u32Number)
			{
				psTerminal->u8Color &= ~0x08;	// Low intensity foreground
			}
			else
			if (4 == u32Number)
			{
				psTerminal->u8Color |= 0x80;	// Set high intensity background
			}
			else
			if (5 == u32Number)
			{
				psTerminal->u8Attribute |= VIDATTR_BLINK;	// Set blink
			}
			else
			if (6 == u32Number)
			{
				psTerminal->u8Color &= ~0x80;	// Set low intensity background
			}
			else
			if (7 == u32Number)
			{
				psTerminal->u8Attribute |= VIDATTR_INVERSE;	// Set inverse
			}
			else
			if (25 == u32Number)
			{
				// Turn blink off
				psTerminal->u8Attribute &= ~VIDATTR_BLINK;
			}
			else
			if (27 == u32Number)
			{
				// Turn invert off
				psTerminal->u8Attribute &= ~VIDATTR_INVERSE;
			}
			else
			if ((u32Number >= 30) && (u32Number <= 37))
			{
				// Set foreground colors
				u32Number -= 30;
				psTerminal->u8Color = (psTerminal->u8Color & 0xf8) | (UINT8) sg_u8ANSIColorMap[u32Number];
			}
			else
			if ((u32Number >= 40) && (u32Number <= 47))
			{
				// Set background colors
				u32Number -= 40;
				psTerminal->u8Color = (psTerminal->u8Color & 0x8f) | (UINT8) (sg_u8ANSIColorMap[u32Number] << 4);
			}
			else
			{
				// Ignore the rest.
				break;
			}

			// Number absorbed
			while (';' == *pu8RXChar)
			{
				++pu8RXChar;
			}

			// Bogus sequence, bail out
			if ((*pu8RXChar < '0')  ||
				(*pu8RXChar > '9'))
			{
				break;
			}
		}
	}

	return(bScrolled);
}

static BOOL TerminalPutCharInternal(STerminal *psTerminal,
									SWindow *psWindow,
									SWidget *psWidget,
									UINT8 u8Char)
{
	BOOL bScrolled = FALSE;

	// Clear blink counter so cursor stops blinking as text is coming across
	psTerminal->u32CursorTimer = 0;

	// And turn it on
	psTerminal->bCursorState = TRUE;

stateCheck:
	switch (psTerminal->eCharState)
	{
		case ECHARSTATE_WAIT_ESCAPE:
		{
			BOOL bResult;

			// If it's not an escape, just print it
			if (u8Char != 0x1b)
			{

				bResult = TerminalPutDisplayableChar(psTerminal,
													 psWindow,
													 psWidget,
													 u8Char);

				if (bResult)
				{
					bScrolled = TRUE;
				}
			}
			else
			{
				// It's an escape. Move to next state
				psTerminal->eCharState = ECHARSTATE_WAIT_BRACKET;
			}

			break;
		}

		case ECHARSTATE_WAIT_BRACKET:
		{
			BOOL bResult = FALSE;

			if (u8Char != '[')
			{
				if (0x1b == u8Char)
				{
					psTerminal->eCharState = ECHARSTATE_WAIT_ESCAPE;
					goto stateCheck;
				}
				else
				{
					// Display the escape character, then whatever this is

					if (TerminalPutDisplayableChar(psTerminal,
												   psWindow,
												   psWidget,
												   0x1b))
					{
						bResult = TRUE;
					}

					if (TerminalPutDisplayableChar(psTerminal,
												   psWindow,
												   psWidget,
												   u8Char))
					{
						bResult = TRUE;
					}

					psTerminal->eCharState = ECHARSTATE_WAIT_ESCAPE;
				}
			}
			else
			{
				// We are absorbing characters due to an escape sequence.
				psTerminal->u32RXLen = 0;
				psTerminal->eCharState = ECHARSTATE_ABSORB_CHARACTERS;
			}
			break;
		}

		case ECHARSTATE_ABSORB_CHARACTERS:
		{
			// If it's an escape character, restart the sequence
			if (0x1b == u8Char)
			{
				psTerminal->eCharState = ECHARSTATE_WAIT_ESCAPE;
				goto stateCheck;
			}
	
			// If this is a space or tab character, just absorb it
			if ((' ' == u8Char) || ('\t' == u8Char))
			{
			}
			else
			{
				// More goodies.
				if (toupper(u8Char) >= 'A' && (toupper(u8Char) <= 'Z'))
				{
					// End of sequence. Time to do something with it
					psTerminal->u8EscapeRXBuffer[psTerminal->u32RXLen] = '\0';
					// Reset the state machine
					psTerminal->eCharState = ECHARSTATE_WAIT_ESCAPE;

					TerminalProcessEscapeSequence(psTerminal,
												  psWindow,
												  psWidget,
												  u8Char);
				}
				else
				{
					psTerminal->u8EscapeRXBuffer[psTerminal->u32RXLen] = u8Char;
					psTerminal->u32RXLen++;

					if (psTerminal->u32RXLen >= sizeof(psTerminal->u8EscapeRXBuffer))
					{
						UINT8 u8Data[sizeof(psTerminal->u8EscapeRXBuffer)];
						UINT32 u32Count = psTerminal->u32RXLen;
						UINT32 u32Loop = 0;

						// Bogus! Just emit the existing data in an on-screen sequence
						psTerminal->eCharState = ECHARSTATE_WAIT_ESCAPE;

						// Note that this is a potentially recursive function, so that's why data in the
						// psTerminal structure is cached in local variables
						memcpy((void *) u8Data, (void *) psTerminal->u8EscapeRXBuffer, sizeof(u8Data));
						while (u32Loop < u32Count)
						{
							(void) TerminalPutDisplayableChar(psTerminal,
													 psWindow,
													 psWidget,
													 u8Data[u32Loop++]);
						}
					}
					else
					{
						// Keep going.
					}
				}
			}

			break;
		}

		default:
		{
			GCASSERT(0);
		}
	}

	return(bScrolled);
}

ELCDErr TerminalPutChar(TERMINALHANDLE eTerminalHandle,
						UINT8 u8Character)
{
	ELCDErr eLCDErr;
	SWidget *psWidget;
	SWindow *psWindow;
	STerminal *psTerminal;
	BOOL bScrolled;

	eLCDErr = WidgetGetPointerByHandle((WIDGETHANDLE) eTerminalHandle,
										WIDGET_TERMINAL,
										&psWidget,
										NULL);

	RETURN_ON_FAIL(eLCDErr);

	psWindow = WindowGetPointer(psWidget->eParentWindow);
	GCASSERT(psWindow);

	psTerminal = psWidget->uWidgetSpecific.psTerminal;
	GCASSERT(psTerminal);

	// If the current cursor is on, get rid of it
	if (psTerminal->bCursorOn)
	{
		TerminalEraseCursor(psTerminal,
							psWindow,
							psWidget);
	}

	bScrolled = TerminalPutCharInternal(psTerminal,
										psWindow,
										psWidget,
										u8Character);

	// Now go redraw the cursor if necessary
	CursorDraw(psWidget,
			   FALSE);

	// Commit the single character change
	WindowUpdateRegionCommit();

	return(eLCDErr);
}

ELCDErr TerminalSetCharCallback(TERMINALHANDLE eTerminalHandle,
								void (*pCallback)(STerminal *psTerminal,
												  LEX_CHAR eChar))
{
	ELCDErr eLCDErr;
	SWidget *psWidget;
	SWindow *psWindow;
	STerminal *psTerminal;

	eLCDErr = WidgetGetPointerByHandle((WIDGETHANDLE) eTerminalHandle,
										WIDGET_TERMINAL,
										&psWidget,
										NULL);

	RETURN_ON_FAIL(eLCDErr);

	psWindow = WindowGetPointer(psWidget->eParentWindow);
	GCASSERT(psWindow);

	psTerminal = psWidget->uWidgetSpecific.psTerminal;
	GCASSERT(psTerminal);

	psTerminal->pCallback = pCallback;

	return(LERR_OK);
}

static UINT8 sg_u8Colors[] =
{
	0,0,0,
	0,0,170,
	0,170,0,
	0,170,170,
	170,0,0,
	170,0,170,
	170,85,0,
	170,170,170,
	85,85,85,
	85,85,255,
	85,255,85,
	85,255,255,
	255,85,85,
	255,85,255,
	255,255,85,
	255,255,255
};

void TerminalFirstTimeInit(void)
{
	UINT8 u8Red;
	UINT8 u8Green;
	UINT8 u8Blue;
	UINT8 u8Loop;
	UINT8 *pu8Color;
	UINT16 u16Combined;

	DebugOut("* Initializing terminal\n");
	WidgetRegisterTypeMethods(WIDGET_TERMINAL,
							  &sg_sTerminalMethods);

	pu8Color = sg_u8Colors;
	for (u8Loop = 0; u8Loop < (sizeof(sg_u8Colors) / 3); u8Loop++)
	{
		u8Red = *pu8Color;
		pu8Color++;
		u8Green = *pu8Color;
		pu8Color++;
		u8Blue = *pu8Color;
		pu8Color++;

		u8Red = (UINT8) (((double) u8Red / (double) 255.0) * 0x1f);
		u8Green = (UINT8) (((double) u8Green / (double) 255.0) * 0x3f);
		u8Blue = (UINT8) (((double) u8Blue / (double) 255.0) * 0x1f);

		u16Combined = (u8Red << 11) | (u8Green << 5) | u8Blue;
		DebugOut("\t0x%.4x,\n", u16Combined);
	}
}